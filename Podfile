# platform :ios, '9.0'

target 'preload' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for preload
  pod ‘SwiftSoup’

  target 'preloadTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'preloadUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
